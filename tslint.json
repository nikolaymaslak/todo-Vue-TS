{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "prefer-const": false,
    "variable-name": false,
    "max-classes-per-file": false,
    "arrow-parens": false,
    "max-line-length": false,
    "no-console": false,
    "no-empty": false,
    "interface-name": true,
    "ordered-imports": true,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": true,
    "trailing-comma": false,
    "semicolon": false,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ]
  }
}
